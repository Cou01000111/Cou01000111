{
    "[astro]": {
        "editor.defaultFormatter": "astro-build.astro-vscode"
    },
    "[c][cpp]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "[css]": {
        "editor.defaultFormatter": "biomejs.biome"
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "biomejs.biome"
    },
    "[jinja-html]": {
        "editor.formatOnSave": false
    },
    "[json][jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[markdown]": {
        "editor.defaultFormatter": "yzhang.markdown-all-in-one",
        "editor.formatOnSave": false,
        "editor.quickSuggestions": {
            "comments": "off",
            "other": "off",
            "strings": "off"
        },
        "files.trimTrailingWhitespace": false
    },
    "[mcfunction]": {
        "editor.defaultFormatter": "SPGoding.datapack-language-server"
    },
    "[nginx]": {
        "editor.defaultFormatter": "raynigon.nginx-formatter"
    },
    "[python]": {
        "editor.formatOnType": true,
        "editor.tabSize": 2
    },
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer"
    },
    "[scss]": {
        "editor.defaultFormatter": "biomejs.biome"
    },
    "[shellscript]": {
        "editor.defaultFormatter": "shakram02.bash-beautify"
    },
    "[sql]": {
        "editor.defaultFormatter": "mtxr.sqltools"
    },
    "[toml]": {
        "editor.defaultFormatter": "tamasfe.even-better-toml"
    },
    "[typescript]": {
        "editor.codeActionsOnSave": {
            "quickfix.biome": "explicit",
            "source.fixAll.eslint": "never",
            "source.organizeImports.biome": "explicit"
        },
        "editor.defaultFormatter": "biomejs.biome",
        "editor.formatOnSave": true
    },
    "[typescriptreact]": {
        "editor.codeActionsOnSave": {
            "quickfix.biome": "explicit",
            "source.organizeImports.biome": "explicit"
        },
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "biomejs.biome"
    },
    "[vue]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "clangd.detectExtensionConflicts": false,
    "clangd.path": "c:\\Users\\coura\\AppData\\Roaming\\Cursor\\User\\globalStorage\\llvm-vs-code-extensions.vscode-clangd\\install\\18.1.3\\clangd_18.1.3\\bin\\clangd.exe",
    "cSpell.ignorePaths": [
        "**/package-lock.json",
        "**/node_modules/**",
        "vscode-extension",
        "**/.git/objects/**",
        ".vscode",
        ".vscode-insiders",
        "myqmk/**",
        "*.json",
        "*.md",
        "**/settings.json"
    ],
    "cSpell.ignoreRegExpList": [
        "[０-９Ａ-Ｚａ-ｚぁ-んァ-ヶ亜-熙纊-黑]+"
    ],
    "cSpell.userWords": [
        "attackable",
        "Behaviour",
        "connectquery",
        "cosense",
        "Hackathon",
        "Lerp",
        "Mathf",
        "popleft",
        "Raycast",
        "Rigidbody",
        "Scrapbox",
        "Toon",
        "userscript",
        "Vite"
    ],
    "cursor.cpp.disabledLanguages": [
        "plaintext",
        "scminput",
        "yaml"
    ],
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.accessibilitySupport": "off",
    "editor.autoClosingBrackets": "always",
    "editor.bracketPairColorization.enabled": true,
    "editor.cursorBlinking": "phase",
    "editor.cursorStyle": "block",
    "editor.cursorSurroundingLines": 20,
    "editor.fontFamily": "Hackgen",
    "editor.fontLigatures": true,
    "editor.formatOnSave": true,
    "editor.inlayHints.enabled": "off",
    "editor.inlineSuggest.enabled": true,
    "editor.inlineSuggest.showToolbar": "always",
    "editor.insertSpaces": true,
    "editor.largeFileOptimizations": false,
    "editor.lineHeight": 1.4,
    "editor.minimap.enabled": false,
    "editor.mouseWheelZoom": true,
    "editor.quickSuggestions": {
        "strings": true
    },
    "editor.quickSuggestionsDelay": 5,
    "editor.renderLineHighlight": "all",
    "editor.renderLineHighlightOnlyWhenFocus": true,
    "editor.renderWhitespace": "all",
    "editor.scrollbar.vertical": "visible",
    "editor.scrollBeyondLastColumn": 20,
    "editor.smoothScrolling": true,
    "editor.stickyScroll.enabled": true,
    "editor.stickyScroll.maxLineCount": 10,
    "editor.suggestSelection": "first",
    "editor.tabSize": 2,
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "editor.unicodeHighlight.invisibleCharacters": false,
    "editor.unicodeHighlight.nonBasicASCII": false,
    "editor.wordBasedSuggestions": "off",
    "editor.wordWrap": "on",
    "editor.wrappingStrategy": "advanced",
    "emmet.showSuggestionsAsSnippets": true,
    "emmet.triggerExpansionOnTab": true,
    "emmet.variables": {
        "lang": "ja"
    },
    "explorer.compactFolders": true,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.fileNesting.patterns": {
        "*.db": "${capture}.${extname}-*",
        "*.db3": "${capture}.${extname}-*",
        "*.s3db": "${capture}.${extname}-*",
        "*.sdb": "${capture}.${extname}-*",
        "*.sqlite": "${capture}.${extname}-*",
        "*.sqlite3": "${capture}.${extname}-*",
        "Cargo.toml": "Cargo.lock"
    },
    "explorer.openEditors.sortOrder": "alphabetical",
    "explorer.sortOrder": "type",
    "files.associations": {
        ".*lintrc": "json",
        "*.c": "c",
        "*.css": "css",
        "*.gs": "javascript"
    },
    "files.autoGuessEncoding": true,
    "files.autoSave": "onFocusChange",
    "files.autoSaveDelay": 1000,
    "files.eol": "\n",
    "files.exclude": {
        "**/*.map": true,
        "**/node_modules": true
    },
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "files.watcherExclude": {
        "**/.ammonite": true,
        "**/.bloop": true,
        "**/.metals": true
    },
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "git.openRepositoryInParentFolders": "never",
    "github.codespaces.showPerformanceExplorer": true,
    "hediet.vscode-drawio.resizeImages": null,
    "html.format.unformatted": null,
    "html.format.wrapLineLength": 0,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "json.schemas": [
        {
            "fileMatch": [
                "config/example/*.json"
            ],
            "url": "./config/schema/schema-example.json"
        }
    ],
    "jumpy.display.backgroundColor": "#111111",
    "jumpy.display.color": "#eeeeee",
    "jumpy.primaryCharset": "jfkdlshgurieowyt",
    "jumpy.useIcons": false,
    "local-history.absolute": true,
    "local-history.path": "~/vscode_history",
    "local-history.saveDelay": 1,
    "markdown-image.base.fileNameFormat": "${timestamp}",
    "markdown.marp.enableHtml": true,
    "markdown.marp.themes": [
        "https://raw.githubusercontent.com/rnd195/my-marp-themes/main/gradient.css",
        "https://raw.githubusercontent.com/rnd195/marp-community-themes/live/themes/graph_paper.css",
        "C:\\Users\\coura\\dev\\",
        "./myTheme.css"
    ],
    "markdown.preview.breaks": true,
    "markdown.preview.fontSize": 25,
    "markdown.preview.lineHeight": 1.4,
    "python.analysis.typeCheckingMode": "strict",
    "redhat.telemetry.enabled": true,
    "remote.autoForwardPortsSource": "hybrid",
    "scm.inputFontSize": 14,
    "search.exclude": {
        "**/tmp": true
    },
    "security.workspace.trust.untrustedFiles": "open",
    "swaggerViewer.previewInBrowser": true,
    "terminal.integrated.commandsToSkipShell": [
        "workbench.view.explorer"
    ],
    "terminal.integrated.fastScrollSensitivity": 0.01,
    "terminal.integrated.fontFamily": "\"HackGen Console NF\"",
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.scrollback": 3000,
    "terminal.integrated.smoothScrolling": true,
    "typescript.tsserver.experimental.enableProjectDiagnostics": true,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "update.mode": "none",
    "update.showReleaseNotes": false,
    "vite.autoStart": false,
    "window.commandCenter": false,
    "window.openFoldersInNewWindow": "on",
    "window.title": "${rootName}",
    "workbench.colorCustomizations": {
        "editor.lineHighlightBackground": "#333333",
        "editorBracketHighlight.foreground1": "#d06293",
        "editorBracketHighlight.foreground2": "#f3cb76",
        "editorBracketHighlight.foreground3": "#5081eb",
        "editorBracketHighlight.foreground4": "#5be7a1",
        "editorBracketHighlight.foreground5": "#a908f3",
        "editorBracketHighlight.foreground6": "#f56b2c"
    },
    "workbench.colorTheme": "Default Dark Modern",
    "workbench.editor.labelFormat": "short",
    "workbench.editor.pinnedTabSizing": "shrink",
    "workbench.editor.tabActionCloseVisibility": false,
    "workbench.editor.tabSizing": "fit",
    "workbench.editorAssociations": {
        "*.db": "sqlite-viewer.option",
        "*.dotm": "default",
        "git-rebase-todo": "gitlens.rebase"
    },
    "workbench.iconTheme": "material-icon-theme",
    "workbench.layoutControl.enabled": false,
    "workbench.sash.size": 7,
    "workbench.statusBar.visible": true,
    "workbench.tree.indent": 18,
    "terminal.integrated.mouseWheelZoom": true,
    "[dockerfile]": {
        "editor.defaultFormatter": "ms-azuretools.vscode-docker"
    },
    "workbench.activityBar.location": "bottom",
